{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.company.com/jobNew.schema.json",
    "type": "object",
    "properties": {
      "title": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "salary": {
        "type": "integer",
        "minimum" : 0
      },
      "equity": {
        "type": "number",
        "pattern": "^\\d+$"
      },
      "companyHandle": {
        "type": "integer",
        "minimum": 0
      }
    },
    "additionalProperties": false,
    "required": [
      "name",
      "handle",
      "description"
    ]
  }